
{
  "identifier": "default",
  "description": "Default permissions for the app",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:allow-write",
      "allow": [{ "path": "**" }]
    },
    {
      "identifier": "fs:allow-create",
      "allow": [{ "path": "**" }]
    },
    "dialog:default",
    "dialog:allow-save"
  ]
}
